{
  "confdir":       "/fake/conf/dir",
  "cloud": {
    "aws": {}
  },
  "mirror": {
    "copies":       2,
    "burst_buffer": 512,
    "util_thresh":  10,
    "optimize_put": false,
    "enabled":      false
  },
  "ec": {
    "objsize_limit": 262144,
    "data_slices": 1,
    "parity_slices": 1,
    "batch_size": 64,
    "compression": "never",
    "enabled": false
  },
  "log": {
    "dir":       "/tmp/ais1/log",
    "level":     "3",
    "max_size":  4194304,
    "max_total": 67108864
  },
  "periodic": {
    "stats_time":        "10s",
    "notif_time":        "30s",
    "retry_sync_time":   "2s"
  },
  "timeout": {
    "max_keepalive":        "4s",
    "cplane_operation":     "2s",
    "send_file_time":       "5m",
    "startup_time":         "1m",
    "max_host_busy":        "1m"
  },
  "client": {
    "client_timeout":      "10s",
    "client_long_timeout": "30m",
    "list_timeout":        "2m"
  },
  "proxy": {
    "non_electable": false,
    "primary_url":   "127.0.0.1",
    "original_url":  "127.0.0.2",
    "discovery_url": "127.0.0.255"
  },
  "lru": {
    "lowwm":             75,
    "highwm":            90,
    "out_of_space":      95,
    "dont_evict_time":   "120m",
    "capacity_upd_time": "10m",
    "enabled":           true
  },
  "disk":{
    "disk_util_low_wm": 20,
    "disk_util_high_wm": 80,
    "disk_util_max_wm": 95,
    "iostat_time_long": "2s",
    "iostat_time_short": "100ms"
  },
  "rebalance": {
    "dont_run_time": "0m",
    "dest_retry_time": "2m",
    "quiescent": "20s",
    "compression": "never",
    "multiplier": 4,
    "enabled": true
  },
  "checksum": {
    "type":                       "xxhash",
    "validate_cold_get":          true,
    "validate_warm_get":          false,
    "validate_cluster_migration": false,
    "enable_read_range":          false
  },
  "versioning": {
    "enabled":           true,
    "validate_warm_get": false
  },
  "fspaths": { },
  "test_fspaths": {
    "root":     "/tmp/ais",
    "count":    0,
    "instance": 0
  },
  "net": {
    "ipv4":               "127.0.1.0",
    "ipv4_intra_control": "127.0.2.0",
    "ipv4_intra_data":    "127.0.3.0",
    "l4": {
      "proto":              "tcp",
      "port":	              "8080",
      "port_intra_control": "9080",
      "port_intra_data":    "10080"
    },
    "http": {
      "server_crt":	"server.crt",
      "server_key":		"server.key",
      "use_https":		false
    }
  },
  "fshc": {
    "enabled":     true,
    "test_files":  4,
    "error_limit": 2
  },
  "auth": {
    "secret":  "FAKESECRETKEY",
    "enabled": false,
    "cred_dir": "/fake/cred/dir"
  },
  "keepalivetracker": {
    "proxy": {
      "interval": "10s",
      "name":     "heartbeat",
      "factor":   3
    },
    "target": {
      "interval": "10s",
      "name":     "heartbeat",
      "factor":   3
    },
    "retry_factor":   5,
    "timeout_factor": 3
  },
  "downloader": {
    "timeout": "1h"
  },
  "distributed_sort": {
    "duplicated_records":    "ignore",
    "missing_shards":        "ignore",
    "ekm_malformed_line":    "abort",
    "ekm_missing_key":       "abort",
    "default_max_mem_usage": "80%",
    "dsorter_mem_threshold": "100GB",
    "call_timeout":          "10m"
  }
}
